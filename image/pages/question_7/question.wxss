/* pages/question5.2.wxss */
.page-row{
  white-space:pre-line;
  /* 换行 */
  width: 250px;
  padding: 0 25px;
  height: 38px;
  /* border: 1px solid #eee5e5; */
  /* background: #f6f6f6; */
  color: #202124;
  font-size: 14px;
  line-height: 48px;
  margin-top: 12px;
  /* border-radius: 25px; */
}
.element{
  float: left;
  width: 25px;
  padding:0 15px;
  height: 38px;
  color: #202124;
  font-size: 18px;
  line-height: 48px;
  display: flex;
}
.fuhao{
  float: left;
  width: 5px;
  padding: 0 15px;
  height: 38px;
  color: #202124;
  font-size: 20px;
  line-height: 48px;
  display: flex;
}
.result{
  float: left;
  width: 55px;
  padding: 0 0 15 0px;
  height: 35px;
  color: #202124;
  font-size: 22px;
  line-height: 10px;
  border: 1px solid #eee5e5;
  text-align: left;
  background: #ffffff;
  display: flex;
}
.button-ensure{
  margin: 300rpx 40px 20px 40px;
  background-color: #0066cc;
  border-radius: 200px;
  color: white;
  font-family: 黑体;
  font-size: x-large;
  text-align: center;
  /* pointer-events: none; */
}
.button-submit {
  margin: 0px 0px 20px 0px;
  background-color: #0066cc;
  border-radius: 200px;
  color: white;
  font-family: 黑体;
  font-size: x-large;
  text-align: center;
}
.textnormal{
  /* border: 1px solid #af3939; */
  height: 30px;
}
button::after {
  border: none;
}

.upper {
  float: left;
  padding-left: 0px;
  padding-top: 5px;
  color: #0000ff;
  border: none;
  background-color: white;
  border-color: white;
}
.play-icon{
  width: 500rpx;
  height: 300rpx;
  border: solid 2rpx rgb(166,166,184);
  border-radius: 5%;
  display: flex;
  margin: 150rpx 40px 20px 120rpx;
}
.play-icon text{
  font-size:100rpx;
  color:#6f6f6f;
  text-align:center;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin:0 0 0 140rpx;
}
.next {
  float: right;
  padding-top: 5px;
  padding-right: 0;
  color: #0000ff;
  border: none;
  background-color: white;
  border-color: white;
}

.dis-upper {
  float: left;
  padding-top: 5px;
  padding-left: 0;
  color: #999ca0;
  background-color: white;
  border-color: white;
  pointer-events: none;
}

.dis-next {
  float: right;
  padding-top: 5px;
  padding-right: 0;
  color: #999ca0;
  background-color: white;
  border-color: white;
  pointer-events: none;
}

.button-icon {
  margin: 180rpx 40px 20px 40px;
  background-color: #0066cc;
  border-radius: 200px;
  color: white;
  font-family: 黑体;
  font-size: x-large;
  text-align: center;
}

.button-dis-icon {
  margin: 180rpx 40px 20px 40px;
  background-color: #999ca0;
  border-radius: 200px;
  color: white;
  font-family: 黑体;
  font-size: x-large;
  text-align: center;
  pointer-events: none;
}